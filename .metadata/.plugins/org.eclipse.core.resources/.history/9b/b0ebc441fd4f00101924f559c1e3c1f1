package Linked;

class Node{
	int data;
	Node next;
	
	Node(int data1, Node next1){
		this.data = data1;
		this.next = next1;
	}
	
	Node(int data1){
		this.data = data1;
		this.next = null;
	}
}

public class LinkedList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = {1,2,3,4};
		Node x = new Node(arr[0]);
		Node y = new Node(arr[1]);
		x.next = y;
		
		System.out.println(x.next.data);

	}

}
